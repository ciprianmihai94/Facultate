x=-pi:0.1:pi;
y=sin(x);
subplot(2,2,1);
plot(x,y,'pb');
xlabel('x');
ylabel('sin(x)'); title('Graficul functiei sin(x)');

x=-pi:0.1:pi;
y=cos(x);
subplot(2,2,2);
plot(x,y,'pb');
xlabel('x');
ylabel('cos(x)'); title('Graficul functiei cos(x)');

x=-1:0.01:1;
y=asin(x);
subplot(2,2,3);
plot(x,y,'pb');
xlabel('x');
ylabel('cos(x)'); title('Graficul functiei arcsin(x)');

x=-1:0.01:1;
y=acos(x);
subplot(2,2,4);
plot(x,y,'pb');
xlabel('x');
ylabel('cos(x)'); title('Graficul functiei arccos(x)');